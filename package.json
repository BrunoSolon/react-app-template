{
  "name": "react-app-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.26.0",
    "react-redux": "^7.2.5",
    "react-scripts": "4.0.3",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "start:dev": "cross-env dotenv -e .env.development craco start",
    "build:dev": "cross-env dotenv -e .env.development craco build && rimraf build/static/js/*.map",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext js,ts,tsx",
    "lint:fix": "npm run eslint --fix src",
    "lint:fixall": "eslint --fix --ext .ts,.tsx ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
